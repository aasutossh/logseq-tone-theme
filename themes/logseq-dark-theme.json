  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5E7A82",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#A8C5CC"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#B8D4DA"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#D4E8ED"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#E8F4F7"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#90B5BD"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#A8C5CC"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#B8D4DA"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#D4E8ED"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#B8D4DA"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#D4E8ED"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#B8D4DA"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A8C5CC"
      }
    },
    {
      "scope": ["punctuation", "punctuation.definition"],
      "settings": {
        "foreground": "#5E7A82"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#E8F4F7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#16A085",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#90B5BD"
      }
    },
    {
      "scope": ["punctuation.definition.list"],
      "settings": {
        "foreground": "#A8C5CC"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#4A6168"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E74C3C",
        "fontStyle": "underline"
      }
    }
