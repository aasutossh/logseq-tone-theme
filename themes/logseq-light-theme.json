  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9CA3AF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#1F2937"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#374151"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "scope": ["punctuation", "punctuation.definition"],
      "settings": {
        "foreground": "#9CA3AF"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#1F2937",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1781E3",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#6B7280"
      }
    },
    {
      "scope": ["punctuation.definition.list"],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#D1D5DB"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#DC2626",
        "fontStyle": "underline"
      }
    }
  ]
}
